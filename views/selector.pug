extends layout

block content
  h1= title
  - var image_names = ["cake1.jpg", "cake2.jpg", "cake3.jpg", "cake4.jpg", "cake5.jpg"];
    - for(let i = 0; i < 3; i++) {
        - let ranValue = Math.floor(Math.random() * 5);
        img(src=`/images/${image_names[ranValue]}`, alt="Cake", height="300px", width="300px")
    - }